{
  "version": 2,
  "routes": [
    {
      "src": "/paiement-reussi/(?<id>[^/]+)",
      "dest": "/paiement-reussi/[id].html"
    },
    {
      "src": "/paiement-echoue/(?<id>[^/]+)",
      "dest": "/paiement-echoue/[id].html"
    },
    {
      "src": "/paiement-reussi/(?<id>[^/]+)/",
      "dest": "/paiement-reussi/[id].html"
    },
    {
      "src": "/paiement-echoue/(?<id>[^/]+)/",
      "dest": "/paiement-echoue/[id].html"
    }
  ],
  "functions": {},
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
